pubic class CodeWordChecker implements StringChecker{

  private int min = 0;
  private int max = 0;
  private String code;
  
  
  public CodeWordChecker(int x int y, string part){
    min = x;
    max = y;
    code = part;
  }
  
  public CodeWordChecker(string part){
   min = 6;
   max = 20;
   code = part;
  }
  
  public boolean isValid (String str){
  for (code.length()<str.length()){
    if(str.length()>=max || str.length()<=min){
      return false;
    }
     else if (str.indexOf(code)!=-1){
      return false;
     }
     else{
      return true;
     }
  }
 }
  
 }
