public class CodeWordChecker implements StringChecker{
    private int min;
    private int max;
    private String string;

    public CodeWordChecker(String str){
        min = 6;
        max = 20
        string = str;
    }

    public CodeWordChecker(int x, int y, String str){
        min = x;
        max = y;
        string = str;
    }

    public boolean isValid(String str){
        return str.length() >= min && str.length() <= max && !str.contains(string);
    }

}
